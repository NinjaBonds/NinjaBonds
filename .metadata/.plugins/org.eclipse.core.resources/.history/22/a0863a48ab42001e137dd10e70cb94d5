package day_01;

public class rearrange {
	
	    public static void main(String arg[]) {
	      String s = "aab";
	      for (int i=0;i<s.length();i++)
	      {   
	    	  int c = 0;
	         char b,d;
	         b = s.charAt(i);
	         d = s.charAt(i+1);
	         if (b != d)

	          c = c + 1;

	      }
	      System.out.println("c is :" + c); 
	      }

	    }

}
